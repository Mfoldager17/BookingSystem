@model IEnumerable<BookingSystemEF.Udlejning>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Opret ny udlejning", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FraDato)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TilDato)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Kunde.Navn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Værktøj.Værktøjsnavn)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FraDato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TilDato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Kunde.Navn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Værktøj.Værktøjsnavn)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.UdlejningsId })
            </td>
        </tr>
    }
</table>